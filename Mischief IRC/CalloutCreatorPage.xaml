                                            <Setter Target="RootGrid.Background" Value="{ThemeResource ToggleButtonRevealBackgroundIndeterminatePressed}"/>
                                            <Setter Target="ContentPresenter.BorderBrush" Value="{ThemeResource ToggleButtonRevealBorderBrushIndeterminatePressed}"/>
                                            <Setter Target="ContentPresenter.Foreground" Value="{ThemeResource ToggleButtonForegroundIndeterminatePressed}"/>
                                        </VisualState.Setters>
                                        <Storyboard>
                                            <PointerDownThemeAnimation Storyboard.TargetName="RootGrid"/>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="IndeterminateDisabled">
                                        <VisualState.Setters>
                                            <Setter Target="RootGrid.Background" Value="{ThemeResource ToggleButtonRevealBackgroundIndeterminateDisabled}"/>
                                            <Setter Target="ContentPresenter.BorderBrush" Value="{ThemeResource ToggleButtonRevealBorderBrushIndeterminateDisabled}"/>
                                            <Setter Target="ContentPresenter.Foreground" Value="{ThemeResource ToggleButtonForegroundIndeterminateDisabled}"/>
                                        </VisualState.Setters>
                                    </VisualState>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                            <ContentPresenter x:Name="ContentPresenter" AutomationProperties.AccessibilityView="Raw" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" ContentTemplate="{TemplateBinding ContentTemplate}" CornerRadius="{TemplateBinding CornerRadius}" Content="{TemplateBinding Content}" ContentTransitions="{TemplateBinding ContentTransitions}" HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}" Padding="{TemplateBinding Padding}" VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style TargetType="TextBlock">
            <Setter Property="FontSize" Value="14"/>
            <Setter Property="VerticalAlignment" Value="Center"/>
            <Setter Property="Margin" Value="5,0"/>
        </Style>
        <Style TargetType="TextBox">
            <Setter Property="IsSpellCheckEnabled" Value="False"/>
            <Setter Property="VerticalAlignment" Value="Center"/>
        </Style>
    </Page.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <ScrollViewer Grid.Row="0">
            <Grid Margin="0,0,0,5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition Width="2*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition Height="2*"/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <!-- Case number -->
                <CheckBox x:Name="CheckBoxCasenumber" Grid.Column="0" Grid.Row="0" Checked="CheckBoxCasenumber_Checked" Unchecked="CheckBoxCasenumber_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Grid.Column="0" Text="#"/>
                        <TextBox x:Name="TextBoxCasenumber" Grid.Column="1" PlaceholderText="Casenumber" Margin="0,0,5,0" TextChanged="TextBoxCasenumber_TextChanged" KeyDown="TextBoxCasenumber_KeyDown"/>
                    </Grid>
                </CheckBox>
                <!-- Acknowledgement -->
                <CheckBox x:Name="CheckBoxRgr" Grid.Column="1" Grid.Row="0" Checked="CheckBoxRgr_Checked" Unchecked="CheckBoxRgr_Unchecked">
                    <TextBlock Text="rgr" Margin="17,0,5,0"/>
                </CheckBox>
                <!-- Readiness confirmation -->
                <CheckBox x:Name="CheckBoxRdy" Grid.Column="2" Grid.Row="0" Checked="CheckBoxRdy_Checked" Unchecked="CheckBoxRdy_Unchecked">
                    <TextBlock Text="rdy" Margin="17,0,5,0"/>
                </CheckBox>
                <!-- Jump callout -->
                <CheckBox x:Name="CheckBoxJumpcallout" Grid.Column="0" Grid.Row="1" Checked="CheckBoxJumpcallout_Checked" Unchecked="CheckBoxJumpcallout_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBox x:Name="TextBoxJumpcallout" Grid.Column="0" PlaceholderText="Number of jumps" Margin="5,0,0,0" TextChanged="TextBoxJumpcallout_TextChanged" KeyDown="TextBoxJumpcallout_KeyDown"/>
                        <TextBlock Grid.Column="1" Text="j"/>
                    </Grid>
                </CheckBox>
                <!-- Own position -->
                <CheckBox x:Name="CheckBoxOwnPos" Grid.Column="1" Grid.Row="1" Checked="CheckBoxOwnPos_Checked" Unchecked="CheckBoxOwnPos_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <RadioButton x:Name="RadioButtonSysPlus" Grid.Column="0" GroupName="OwnPos" Checked="RadioButtonSysPlus_Checked">
                            <TextBlock Text="sys+"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonPosPlus" Grid.Column="1" GroupName="OwnPos" Checked="RadioButtonPosPlus_Checked">
                            <TextBlock Text="pos+"/>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- Friend request status -->
                <CheckBox x:Name="CheckBoxFr" Grid.Column="0" Grid.Row="2" Checked="CheckBoxFr_Checked" Unchecked="CheckBoxFr_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <RadioButton x:Name="RadioButtonFrPlus" Grid.Column="0" GroupName="Fr" Checked="RadioButtonFrPlus_Checked">
                            <TextBlock Text="fr+"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonFrMinus" Grid.Column="1" GroupName="Fr" Checked="RadioButtonFrMinus_Checked">
                            <TextBlock Text="fr-"/>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- Client online status -->
                <CheckBox x:Name="CheckBoxClientOnlineStatus" Grid.Column="1" Grid.Row="2" Checked="CheckBoxClientOnlineStatus_Checked" Unchecked="CheckBoxClientOnlineStatus_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <TextBlock x:Name="TextBlockClientOnlineStatus" Grid.ColumnSpan="2" Grid.RowSpan="2" HorizontalAlignment="Center" Text="client ..."/>
                        <RadioButton x:Name="RadioButtonClientInOpen" Grid.Column="0" Grid.Row="0" GroupName="ClientOnlinestatus" Checked="RadioButtonClientInOpen_Checked">
                            <TextBlock Text="in open"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonClientInPg" Grid.Column="1" Grid.Row="0" GroupName="ClientOnlinestatus" Checked="RadioButtonClientInPg_Checked">
                            <TextBlock Text="in pg"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonClientInSolo" Grid.Column="0" Grid.Row="1" GroupName="ClientOnlinestatus" Checked="RadioButtonClientInSolo_Checked">
                            <TextBlock Text="in solo"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonClientInMm" Grid.Column="1" Grid.Row="1" GroupName="ClientOnlinestatus" Checked="RadioButtonClientInMm_Checked">
                            <TextBlock Text="in mm"/>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- System confirmation / correction -->
                <CheckBox x:Name="CheckBoxSysconf" Grid.Column="2" Grid.Row="2" Checked="CheckBoxSysconf_Checked" Unchecked="CheckBoxSysconf_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="2*"/>
                        </Grid.ColumnDefinitions>
                        <RadioButton x:Name="RadioButtonSysconf" Grid.Column="0" GroupName="Sysconf" Checked="RadioButtonSysconf_Checked">
                            <TextBlock Text="sysconf"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonSyscorr" Grid.Column="1" GroupName="Sysconf" Checked="RadioButtonSyscorr_Checked">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="syscorr:"/>
                                <TextBox x:Name="TextBoxSyscorr" Grid.Column="1" PlaceholderText="Actual system" Margin="0,0,5,0" TextChanged="TextBoxSyscorr_TextChanged" KeyDown="TextBoxSyscorr_KeyDown"/>
                            </Grid>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- Team status -->
                <CheckBox x:Name="CheckBoxTm" Grid.Column="0" Grid.Row="3" Checked="CheckBoxTm_Checked" Unchecked="CheckBoxTm_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <RadioButton x:Name="RadioButtonTmPlus" Grid.Column="0" GroupName="Tm" Checked="RadioButtonTmPlus_Checked">
                            <TextBlock Text="tm+"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonTmMinus" Grid.Column="1" GroupName="Tm" Checked="RadioButtonTmMinus_Checked">
                            <TextBlock Text="tm-"/>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- Preparation status -->
                <CheckBox x:Name="CheckBoxPrep" Grid.Column="1" Grid.Row="3" Checked="CheckBoxPrep_Checked" Unchecked="CheckBoxPrep_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <RadioButton x:Name="RadioButtonPrepPlus" Grid.Column="0" GroupName="Prep" Checked="RadioButtonPrepPlus_Checked">
                            <TextBlock Text="prep+"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonPrepMinus" Grid.Column="1" GroupName="Prep" Checked="RadioButtonPrepMinus_Checked">
                            <TextBlock Text="prep-"/>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- Navigation panel check -->
                <CheckBox x:Name="CheckBoxNavcheck" Grid.Column="2" Grid.Row="3" Checked="CheckBoxNavcheck_Checked" Unchecked="CheckBoxNavcheck_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <RadioButton x:Name="RadioButtonNavcheckCanNotJump" Grid.Column="0" GroupName="Navcheck" Checked="RadioButtonNavcheckCanNotJump_Checked">
                            <TextBlock Text="navcheck: client&#10;can not jump"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonNavcheckCanJump" Grid.Column="1" GroupName="Navcheck" Checked="RadioButtonNavcheckCanJump_Checked">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="auto"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="auto"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="can jump"/>
                                <TextBox x:Name="TextBoxNavcheckCanJump" Grid.Column="1" PlaceholderText="Distance" TextChanged="TextBoxNavcheckCanJump_TextChanged" KeyDown="TextBoxNavcheckCanJump_KeyDown"/>
                                <TextBlock Grid.Column="2" Text="ly"/>
                            </Grid>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- Beacon status -->
                <CheckBox x:Name="CheckBoxBc" Grid.Column="0" Grid.Row="4" Checked="CheckBoxBc_Checked" Unchecked="CheckBoxBc_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <RadioButton x:Name="RadioButtonBcPlus" Grid.Column="0" GroupName="Bc" Checked="RadioButtonBcPlus_Checked">
                            <TextBlock Text="bc+"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonBcMinus" Grid.Column="1" GroupName="Bc" Checked="RadioButtonBcMinus_Checked">
                            <TextBlock Text="bc-"/>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- Instance status -->
                <CheckBox x:Name="CheckBoxInst" Grid.Column="0" Grid.Row="5" Checked="CheckBoxInst_Checked" Unchecked="CheckBoxInst_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <RadioButton x:Name="RadioButtonInstPlus" Grid.Column="0" GroupName="Inst" Checked="RadioButtonInstPlus_Checked">
                            <TextBlock Text="inst+"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonInstMinus" Grid.Column="1" GroupName="Inst" Checked="RadioButtonInstMinus_Checked">
                            <TextBlock Text="inst-"/>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- Client position -->
                <CheckBox x:Name="CheckBoxClientPos" Grid.Column="1" Grid.Row="5" Checked="CheckBoxClientPos_Checked" Unchecked="CheckBoxClientPos_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="3*"/>
                            <ColumnDefinition Width="2*"/>
                        </Grid.ColumnDefinitions>
                        <RadioButton x:Name="RadioButtonClientPosInEz" Grid.Column="0" GroupName="ClientPos" Checked="RadioButtonClientPosInEz_Checked">
                            <TextBlock Text="client in ez"/>
                        </RadioButton>
                        <RadioButton x:Name="RadioButtonClientPosInSc" Grid.Column="1" GroupName="ClientPos" Checked="RadioButtonClientPosInSc_Checked">
                            <TextBlock Text="in sc"/>
                        </RadioButton>
                    </Grid>
                </CheckBox>
                <!-- Distance -->
                <CheckBox x:Name="CheckBoxDistance" Grid.Column="2" Grid.Row="5" Checked="CheckBoxDistance_Checked" Unchecked="CheckBoxDistance_Unchecked">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition Width="auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBox x:Name="TextBoxDistance" Grid.Column="0" PlaceholderText="Distance" Margin="5,0,0,0" TextChanged="TextBoxDistance_TextChanged" PreviewKeyDown="TextBoxDistance_PreviewKeyDown" KeyDown="TextBoxDistance_KeyDown"/>
                        <ComboBox x:Name="ComboBoxDistance" Grid.Column="1" Margin="0,0,5,0" Loaded="ComboBoxDistance_Loaded" SelectionChanged="ComboBoxDistance_SelectionChanged"/>
                    </Grid>
                </CheckBox>
                <!-- Fuel status -->
                <CheckBox x:Name="CheckBoxFuelPlus" Grid.Column="0" Grid.Row="6" Checked="CheckBoxFuelPlus_Checked" Unchecked="CheckBoxFuelPlus_Unchecked">
                    <TextBlock Text="fuel+" Margin="17,0,5,0"/>
                </CheckBox>
                <!-- Show case / case list / Reset -->
                <Grid Grid.Column="2" Grid.Row="6" HorizontalAlignment="Right" VerticalAlignment="Bottom">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                        <ColumnDefinition Width="auto"/>
                    </Grid.ColumnDefinitions>
                    <Button x:Name="ButtonShowCase" Grid.Column="0" Content="Show case" Margin="5,0,0,5" IsEnabled="False" Style="{ThemeResource ButtonRevealStyle}" Click="ButtonShowCase_Click"/>
                    <Button x:Name="ButtonShowCaseList" Grid.Column="1" Content="Show case list" Margin="5,0,0,5" IsEnabled="False" Style="{ThemeResource ButtonRevealStyle}" Click="ButtonShowCaseList_Click"/>
                    <Button x:Name="ButtonReset" Grid.Column="2" Content="Reset" Margin="5,0,5,5" Style="{ThemeResource ButtonRevealStyle}" Click="ButtonReset_Click"/>
                </Grid>
            </Grid>
        </ScrollViewer>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition Width="auto"/>
            </Grid.ColumnDefinitions>
            <TextBox x:Name="TextBoxMessage" Grid.Column="0" KeyDown="TextBoxMessage_KeyDown"/>
            <Button x:Name="ButtonSend" Grid.Column="1" Margin="12,0,0,0" Style="{ThemeResource ButtonRevealStyle}" Click="ButtonSend_Click">
                <SymbolIcon Symbol="Send"/>
            </Button>
        </Grid>
    </Grid>
</Page>
